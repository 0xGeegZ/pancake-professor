{
  "name": "pancake-professor",
  "version": "0.0.2",
  "title": "Pancake Professor",
  "description": "Play to pancake prediction like the best players",
  "author": {
    "name": "GeegZ"
  },
  "private": true,
  "scripts": {
    "test": "ts-node worker/scripts/launch-strategies/index.js",
    "script": "ts-node",
    "predev": "yarn run generate",
    "dev": "next dev",
    "prebuild": "yarn run generate",
    "build": "next build",
    "start": "next start",
    "start:ci": "INSECURE_AUTH=true yarn run start",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier:watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "eslint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache --fix",
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "yarn run eslint . --fix",
    "lint:next": "next lint --fix",
    "typechain": "typechain --target ethers-v5 --outDir ./src/client/contracts/types './src/client/contracts/*.json'",
    "generate": "yarn run generate:prisma && yarn run generate:nexus && yarn run generate:genql && yarn run generate:graphql-codegen",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env GENERATE=true cross-env DOTENV_CONFIG_PATH=.env ts-node --require dotenv/config --transpile-only -P nexus.tsconfig.json src/server/graphql/schema.ts",
    "generate:genql": "cross-env GENERATE=true cross-env DOTENV_CONFIG_PATH=.env genql --require dotenv/config --schema ./src/server/graphql/schema.graphql --output ./src/client/graphql/generated/genql  --watch 'src/server/graphql/schema.graphql'",
    "generate:graphql-codegen": "graphql-codegen",
    "prisma:migrate": "yarn prisma migrate dev --preview-feature",
    "prisma:generate:prod": "yarn prisma db push",
    "prisma:migrate:prod": "DATABASE_URL=$MIGRATION_DATABASE_URL yarn prisma migrate dev --preview-feature",
    "prisma:studio": "prisma studio",
    "typecheck": "concurrently --raw yarn:typecheck:*",
    "typecheck:app": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "post-update": "yarn upgrade --latest"
  },
