{
  "name": "pancakeswap-prediction-bot",
  "version": "1.0.1",
  "description": "This bot wins almost every 5 minute BNB-USD option on PancakeSwap",
  "main": "index.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "launch:multiple:players": "cross-env node -e 'require(\"./server/jobs/strategies/multiple-players/index.js\").run()'",
    "launch:single:auto": "cross-env node -e 'require(\"./server/jobs/strategies/single-player-auto/index.js\").run()'",
    "launch:strategies": "cross-env node -e 'require(\"./server/scripts/launch-strategies/index.js\").launchStrategies()'",
    "claim:strategies": "cross-env node -e 'require(\"./server/scripts/claim-strategies/index.js\").claimStrategies()'",
    "stop:strategies": "cross-env node -e 'require(\"./server/scripts/stop-strategies/index.js\").stopStrategies()'",
    "generate": "prisma generate",
    "dev:multiple": "yarn run 'launch:multiple:players'",
    "dev:single:auto": "yarn run 'launch:single:auto'",
    "dev": "yarn run 'stop:strategies' &&  yarn run 'launch:strategies'",
    "dev:pm2": "pm2 start \"yarn run 'stop:strategies' &&  yarn run 'launch:strategies'\"",
    "dev:pm2:auto": "pm2 start \"yarn run 'launch:single:auto'\"",
    "dev:pm2:logs": "yarn run 'stop:strategies' && (pm2 start \"yarn run 'launch:strategies'\" &) && yarn run 'pm2:logs'",
    "dev:pm2:logs:error": "(pm2 start \"yarn run 'stop:strategies' &&  yarn run 'launch:strategies'\" &) && yarn run 'pm2:logs'",
    "dev:start": "yarn run 'stop:strategies' &&  (yarn run 'launch:strategies' &)",
    "dev:stop": "killall node",
    "start": "node pm2.js",
    "start:logs": "node pm2.js && yarn run 'pm2:logs'",
    "stop": "yarn run 'pm2:stop:all' && yarn run 'pm2:list'",
    "pm2:stop": "yarn run 'pm2:stop:all' && yarn run 'pm2:delete:all'",
    "pm2:list": "pm2 list",
    "pm2:logs": "pm2 logs --lines 500",
    "pm2:monit": "pm2 monit",
    "pm2:stop:all": "pm2 stop all",
    "pm2:delete:all": "pm2 delete all",
    "pm2:stop:listen": "pm2 stop listen",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier:watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "eslint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache --fix",
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "yarn run eslint . --fix",
    "lint:next": "next lint --fix"
  },
  "dependencies": {
    "@prisma/client": "^2.23.0",
    "@supercharge/promise-pool": "^2.2.0",
    "bnc-sdk": "^4.2.0",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "ethers": "^5.6.8",
    "ethers-multicall": "^0.2.3",
    "graphql-request": "^4.2.0",
    "pm2": "^5.2.0",
    "prisma": "^2.23.0",
    "puppeteer": "^13.1.3",
    "winston": "^3.3.3",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14"
  },
  "packageManager": "yarn@3.1.0",
  "prisma": {
    "schema": "server/db/schema.prisma"
  }
}
